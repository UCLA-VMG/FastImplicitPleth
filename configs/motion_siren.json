{
	"device": "cuda",
	"data":{
		"batch_size":16384,
		"trace_batch_size":16384,
		"start_frame":80,
		"num_frames":64,
		"shuffle":true,
		"norm_value": 255
	},
	"train": {
		"epochs": 10
	},
	"opt": {
		"otype": "Adam",
		"lr": 1e-2,
		"beta1": 0.9,
		"beta2": 0.99,
		"eps": 1e-15,
		"l2_reg": 1e-6
	},
	"encoding": {
		"otype": "HashGrid",
		"input_dims": 2,
		"n_levels": 32,
		"n_features_per_level": 2,
		"log2_hashmap_size": 22,
		"base_resolution": 16,
		"per_level_scale": 1.5
	},
	"time_network": {
		"otype": "CutlassMLP",
		"activation": "Sine",
		"output_activation": "None",
		"n_neurons": 64,
		"n_hidden_layers": 2,
		"output_dims": 20
	},
	"spatial_network": {
		"input_dims": 2,
		"hidden_dims": 64,
		"n_hidden": 1,
		"outermost_linear": true,
		"output_dims": 3,
		"first_omega_0": 1,
		"hidden_omega_0": 1.0,
		"L_min": 0,
		"L_max": 10
	},
	"ckpt_save": {
		"dir": null,
		"file": "epoch_",
		"ext": ".avi"
	},
	"trace": {
		"plot": false,
		"folder": "motion",
		"file_tag": "epoch_",
		"ext": ".avi"
	}
}
