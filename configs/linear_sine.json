{
	"device": "cuda",
	"data":{
		"batch_size":16384,
		"trace_batch_size":16384,
		"start_frame":80,
		"num_frames":64,
		"shuffle":true,
		"norm_value": 255
	},
	"train": {
		"epochs": 10
	},
	"opt": {
		"otype": "Adam",
		"lr": 1e-2,
		"beta1": 0.9,
		"beta2": 0.99,
		"eps": 1e-15,
		"l2_reg": 1e-6
	},
	"encoding": {
		"otype": "HashGrid",
		"input_dims": 3,
		"n_levels": 32,
		"n_features_per_level": 2,
		"log2_hashmap_size": 22,
		"base_resolution": 16,
		"per_level_scale": 1.5
	},
	"network": {
		"hidden_dims": 64,
		"output_dims": 3
	},
	"ckpt_save": {
		"dir": null,
		"file": "epoch_",
		"ext": ".pth"
	},
	"trace": {
		"plot": false,
		"folder": "linear_sine_videos",
		"file_tag": "epoch_",
		"ext": ".avi"
	}
}
