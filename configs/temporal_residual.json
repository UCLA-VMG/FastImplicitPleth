{
	"spatiotemporal_device": "cuda:0",
	"deltaspatial_device": "cuda:0",
	"pleth_device": "cuda:1",
	"io_device": "cuda:1",
	"data":{
		"batch_size":8192,
		"trace_batch_size":8192,
		"start_frame":80,
		"num_frames":300,
		"shuffle":true,
		"norm_value": 255
	},
	"train": {
		"epochs": 100
	},
	"opt": {
		"otype": "Adam",
		"lr": 1e-4,
		"beta1": 0.9,
		"beta2": 0.99,
		"eps": 1e-15,
		"l2_reg": 1e-6
	},
	"appearance_model": {
		"config": "configs/cascaded_appearance.json",
		"load_path": "v_84_2_appearance/appearance_model.pth"
	},
	"pleth_encoding": {
		"otype": "HashGrid",
		"input_dims": 2,
		"n_levels": 8,
		"n_features_per_level": 2,
		"log2_hashmap_size": 24,
		"base_resolution": 16,
		"per_level_scale": 1.5
	},
	"pleth_network": {
		"otype": "CutlassMLP",
		"activation": "Sine",
		"output_activation": "none",
		"n_neurons": 64,
		"n_hidden_layers": 2,
		"output_dims": 3
	},
	"checkpoints": {
		"save": false,
		"dir": "temporal",
		"epoch_frequency": 1,
		"file_tag": "epoch_",
		"latest": "latest.pth",
		"ext": ".pth"
	},
	"trace": {
		"plot": false,
		"folder": "temporal__",
		"file_tag": "epoch_",
		"trace_epoch": 1,
		"ext": ".avi"
	}
}
