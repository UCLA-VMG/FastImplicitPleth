{
	"device": "cuda",
	"data":{
		"batch_size":16384,
		"trace_batch_size":16384,
		"start_frame":80,
		"num_frames":300,
		"shuffle":true,
		"norm_value": 255
	},
	"train": {
		"epochs": 100
	},
	"opt": {
		"otype": "Adam",
		"lr": 1e-2,
		"beta1": 0.9,
		"beta2": 0.99,
		"eps": 1e-15,
		"l2_reg": 1e-6
	},
	"time_encoding": {
		"otype": "HashGrid",
		"input_dims": 3,
		"n_levels": 8,
		"n_features_per_level": 2,
		"log2_hashmap_size": 24,
		"base_resolution": 16,
		"per_level_scale": 1.5
	},
	"time_network": {
		"otype": "FullyFusedMLP",
		"activation": "ReLU",
		"output_activation": "tanh",
		"n_neurons": 64,
		"n_hidden_layers": 2,
		"output_dims": 2
	},
	"spatial_encoding": {
		"otype": "HashGrid",
		"input_dims": 2,
		"n_levels": 8,
		"n_features_per_level": 2,
		"log2_hashmap_size": 24,
		"base_resolution": 16,
		"per_level_scale": 1.5
	},
	"spatial_network": {
		"otype": "FullyFusedMLP",
		"activation": "ReLU",
		"output_activation": "none",
		"n_neurons": 64,
		"n_hidden_layers": 2,
		"output_dims": 3
	},
	"ckpt_save": {
		"dir": null,
		"file": "epoch_",
		"ext": ".avi"
	},
	"trace": {
		"plot": false,
		"folder": "motion_300_delta_3dimoffset_8nlevels_tanh",
		"file_tag": "epoch_",
		"ext": ".avi"
	}
}
